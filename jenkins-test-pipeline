pipeline {
  agent any
  
  stages {
    stage ("TEST") {
      parallel {
        stage ("Clone") {
          input {
            message "continue after end of log"
            id "CLONE"
            submitter "svcPipelineBatchBuilder"
            parameters {
               string(name: 'result', defaultValue: 'notok', description: 'how did the clone run?')
            }
          }
          steps {
            echo "Clone was ${result}"
          }
        }
        stage ("3") {
          steps {
            sh "echo Hello"
          }
        }
      }
    }
  }
}     

pipeline {
  agent any
  stages {
    stage ("TEST") {
      parallel {
        stage ("1") {
          steps {
            echo "Test 1"
          }
        }
        stage ("2") {
          steps {
            echo "Test 2"
          }
        }
        stage ("3") {
          steps {
            sh "echo Hello"
          }
        }
      }
    }
  }
}     

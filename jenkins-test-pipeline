pipeline {
  agent any
  
  stages {
    stage ("TEST") {
      parallel {
        stage ("Clone") {
          try {
            input {
              message "continue after end of log"
              id "CLONE"
              submitter "svcPipelineBatchBuilder"
            }
          } catch (err) {
            echo "not interested in errors"
          }
          steps {
            echo "Clone Stage Step"
          }
        }
        stage ("other") {
          steps {
            sh "echo Hello"
          }
        }
      }
    }
  }
}     

pipeline {
  agent any
  
  stages {
    stage ("TEST") {
      parallel {
        stage ("1") {
          input {
            message "continue after end of clone"
            id "CLONE"
            submitter "svcPipelineBatchBuilder"
          }
          steps {
            echo "continue"
          }
        }
        stage ("2") {
          input {
            message "continue after end of log"
            id "LOG"
            submitter "svcPipelineBatchBuilder"
          }
          steps {
            echo "Test 2"
          }
        }
        stage ("3") {
          steps {
            sh "echo Hello"
          }
        }
      }
    }
  }
}     
